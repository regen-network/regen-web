{
  "name": "web-core",
  "version": "2.6.0",
  "private": true,
  "main": "build/index.js",
  "scripts": {
    "remove-previous-files": "shx rm -rf ./build/*",
    "watch": "bun run remove-previous-files && tsc --watch",
    "build": "bun run remove-previous-files && tsc",
    "lint": "eslint ./src  -c .eslintrc.js --max-warnings 0 --ext .ts,.tsx",
    "lint-fix": "eslint ./src --fix -c .eslintrc.js --ext .ts,.tsx",
    "format": "prettier --write --loglevel warn --ignore-path=../.prettierignore './src/**/*.{ts,tsx,json,md,css}'",
    "format-and-fix": "bun run format && bun run lint-fix",
    "graphql:codegen-sanity": "graphql-codegen --config sanity-codegen.yml",
    "test": "jest"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "dependencies": {
    "@analytics/amplitude": "^0.1.3",
    "@analytics/google-analytics": "^1.0.7",
    "@apollo/client": "^3.4.17",
    "@cosmjs/proto-signing": "0.28.13",
    "@emotion/react": "^11.10.4",
    "@emotion/styled": "^11.10.4",
    "@mapbox/mapbox-sdk": "^0.15.3",
    "@regen-network/api": "0.6.0",
    "@tanstack/react-query": "^4.2.3",
    "@types/react": "^18.0.23",
    "@types/react-dom": "^18.0.7",
    "dayjs": "^1.10.7",
    "immer": "^9.0.19",
    "jotai": "^2.0.1",
    "jotai-immer": "^0.2.0",
    "jsonld": "5.2.0",
    "lodash": "^4.17.21",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "stream": "^0.0.2",
    "tss-react": "^4.4.1"
  },
  "devDependencies": {
    "@types/mapbox__mapbox-sdk": "^0.13.6",
    "@types/jest": "^29.0.0",
    "clsx": "^2.0.0",
    "csstype": "^3.1.1",
    "eslint": "^8.42.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "jest": "^28.1.3",
    "prettier": "2.4.1",
    "tailwind-merge": "^1.14.0",
    "ts-jest": "^28.0.8"
  }
}
