{
  "extends": "../tsconfig.json",
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "isolatedModules": true,
    "outDir": "build",
    "resolveJsonModule": true,
    "rootDir": "src",
    "allowJs": true,
    "jsx": "preserve",
    "lib": ["dom", "dom.iterable", "esnext"],
    "module": "esnext",
    "noEmit": true
  },
  "include": [
    "src",
    "types/react-app-env.d.ts",
    "types/rdf-validate-shacl/index.d.ts",
    "types/url-search-params-polyfill.d.ts"
  ]
}
